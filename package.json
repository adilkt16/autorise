{
  "name": "autoriseapp",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev:android": "expo start --dev-client --android",
    "dev:ios": "expo start --dev-client --ios",
    "build:android": "eas build --platform android --profile development",
    "build:ios": "eas build --platform ios --profile development",
    "run:android": "npx expo run:android",
    "run:ios": "npx expo run:ios",
    "prebuild": "npx expo prebuild --clean"
  },
  "dependencies": {
    "expo": "~53.0.22",
    "expo-audio": "~0.4.9",
    "expo-av": "~15.1.7",
    "expo-background-fetch": "~13.1.6",
    "expo-dev-client": "~5.2.4",
    "expo-keep-awake": "~14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-notifications": "~0.31.4",
    "expo-status-bar": "~2.2.3",
    "expo-task-manager": "~13.1.6",
    "react": "19.0.0",
    "react-native": "0.79.6",
    "web-streams-polyfill": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/node": "^24.3.1",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.73.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
